{
    "name": "setharo/setharo",
    "description": "Setharo Laravel client for sending errors and system messages to Setharo monitoring platform.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Harold Peter Ssebetta",
            "email": "haroldpeter6@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Setharo\\": "src/"
        },
        "files": [
            "helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Setharo\\SetharoServiceProvider"
            ],
            "aliases": {
                "Setharo": "Setharo\\Facades\\Setharo"
            }
        }
    },
    "prefer-stable": true
}
